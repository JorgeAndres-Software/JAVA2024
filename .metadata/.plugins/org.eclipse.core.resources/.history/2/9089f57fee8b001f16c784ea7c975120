package com.epn;

public class TestRectangulo {

	public static void main(String[] args) {
		Rectangulo r1=new Rectangulo();
		Rectangulo r2=new Rectangulo();
		int area1;
		int area2;
		
		r1.base=10;
		r1.altura=5;
		
		area1=r1.calcularArea();
		area2=r2.calcularArea();
		
		System.out.println("El area: "+area1);

	}

}
